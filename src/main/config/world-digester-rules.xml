<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE digester-rules PUBLIC
   "-//Jakarta Apache //DTD digester-rules XML V1.0//EN"
   "digester-rules.dtd">
<digester-rules>
	<object-create-rule pattern="rooms" classname="java.util.ArrayList" />
	<object-create-rule pattern="rooms/room"
		classname="javamud.room.SimpleRoom" />
		<bean-property-setter-rule pattern="rooms/room/title"
		propertyname="title" />
	<bean-property-setter-rule pattern="rooms/room/description"
		propertyname="description" />
	<bean-property-setter-rule pattern="rooms/room/id"
		propertyname="id" />
	<set-next-rule pattern="rooms/room" methodname="add" />
</digester-rules>